{
  "name": "yokubo-backend",
  "version": "1.0.0",
  "description": "Backend of Yokubo, a task management tool",
  "main": "dist/src/api/index.js",
  "author": "Markus HÃ¶sel",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rimraf ./dist && ./node_modules/.bin/tsc -p .",
    "start": "node ./dist/src/index.js | ./node_modules/.bin/bunyan",
    "db:migrate": "node ./dist/src/scripts/db/migrate.js | ./node_modules/.bin/bunyan",
    "db:migrate-up": "node ./dist/src/scripts/db/migrateUp.js | ./node_modules/.bin/bunyan",
    "db:migrate-down": "node ./dist/src/scripts/db/migrateDown.js | ./node_modules/.bin/bunyan",
    "db:drop-tables": "node ./dist/src/scripts/db/dropTables.js | ./node_modules/.bin/bunyan",
    "test": "export NODE_ENV=test && mocha ./dist/src/test/setup.js --exit --colors --timeout 60000 --recursive \"./dist/src/**/*.test.js\" | ./node_modules/.bin/bunyan",
    "test:update": "export CHAI_JEST_SNAPSHOT_UPDATE_ALL=true && yarn test",
    "test:sendMails": "export SEND_TEST_MAILS=true && yarn test"
  },
