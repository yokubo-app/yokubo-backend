{
  "name": "yokubo-backend",
  "version": "1.0.0",
  "description": "Backend of Yokubo, a task management tool",
  "main": "dist/src/api/index.js",
  "author": "Markus HÃ¶sel",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "./node_modules/.bin/tsc -p .",
    "start": "source ./env/env.sh && node ./dist/src/api/index.js | ./node_modules/.bin/bunyan",
    "test": "source ./env/test.sh && mocha ./dist/src/test/setup.js --colors --timeout 60000 --recursive \"./dist/src/**/*.test.js\" | ./node_modules/.bin/bunyan",
    "test:update": "source ./env/test.sh && export CHAI_JEST_SNAPSHOT_UPDATE_ALL=true && mocha ./dist/src/test/setup.js -c -t 30000 | ./node_modules/.bin/bunyan",
    "db:migrate": "source ./env/env.sh && node ./dist/src/scripts/db/migrate.js | ./node_modules/.bin/bunyan",
    "db:migrate-up": "source ./env/env.sh && node ./dist/src/scripts/db/migrateUp.js | ./node_modules/.bin/bunyan",
    "db:migrate-down": "source ./env/env.sh && node ./dist/src/scripts/db/migrateDown.js | ./node_modules/.bin/bunyan",
    "db:drop-tables": "source ./env/env.sh && node ./dist/src/scripts/db/dropTables.js | ./node_modules/.bin/bunyan"
  },
  "dependencies": {
    "@types/chai": "^4.1.2",
    "@types/traverse": "^0.6.30",
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "boom": "^7.2.0",
    "bunyan": "^1.8.12",
    "cls-bluebird": "^2.1.0",
    "continuation-local-storage": "^3.2.1",
    "hapi": "^17.2.3",
    "hapi-auth-basic": "^5.0.0",
    "hapi-auth-bearer-token": "^6.0.1",
    "inert": "^5.1.0",
    "joi": "^13.1.2",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "pg-native": "^2.2.0",
    "reflect-metadata": "^0.1.12",
    "sequelize": "^4.37.1",
    "sequelize-decorators": "^1.0.2",
    "sequelize-typescript": "^0.6.3",
    "source-map-support": "^0.5.4",
    "traverse": "^0.6.6",
    "umzug": "^2.1.0",
    "uuid": "^3.2.1",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/bluebird": "^3.5.20",
    "@types/bluebird-global": "^3.5.5",
    "@types/boom": "^7.2.0",
    "@types/bunyan": "^1.8.4",
    "@types/chai-jest-snapshot": "^1.3.2",
    "@types/continuation-local-storage": "^3.2.1",
    "@types/hapi": "^17.0.6",
    "@types/inert": "^4.2.7",
    "@types/joi": "^13.0.6",
    "@types/lodash": "^4.14.105",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.7",
    "@types/pg": "^7.4.5",
    "@types/require-dir": "^0.3.0",
    "@types/sequelize": "^4.27.10",
    "@types/source-map-support": "^0.4.0",
    "@types/umzug": "^2.1.1",
    "@types/uuid": "^3.4.3",
    "@types/zxcvbn": "^4.4.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "chai-jest-snapshot": "^2.0.0",
    "mocha": "^5.0.4",
    "require-dir": "^1.0.0",
    "tslint": "^5.9.1",
